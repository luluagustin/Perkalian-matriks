  cout << " \t >>>>>>>>>>>PERKALIAN MATRIKS<<<<<<<<<<<<<" << endl << endl;

  int matriks1[50][50];
  int (*pM1)[50] = matriks1;
  int matriks2[50][50];
  int (*pM2)[50] = matriks2;
